{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "babel-node buildScripts/srcServer.js lint:watch",
    "lint": "esw src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "preprod": "npm-run-all clean-dist lint",
    "prod": "babel-node buildScripts/build.js",
    "postprod": "babel-node buildScripts/distServer.js"
  },
  "dependencies": {
    "bulma": "^0.2.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.24.1",
    "localtunnel": "^1.8.2",
    "node-sass": "^4.1.1",
    "npm-run-all": "^3.1.2",
    "open": "^0.0.5",
    "postcss-loader": "^1.2.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "webpack-md5-hash": "^0.0.5"
  }
}
